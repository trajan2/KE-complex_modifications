{
    "title": "Florian Script Modification",
    "rules": [
      {"description": "Dismiss Window",
            "manipulators": [
                {
                    "conditions": [
                        {
                            "name": "caps_lock pressed",
                            "type": "variable_if",
                            "value": 1
                        }
                    ],
                    "from": {
                        "key_code": "1",
                        "modifiers": {
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "osascript ~/.config/karabiner/applescripts/auto_dismiss.scpt"
                        }
                    ],
                    "type": "basic"
                }
            ]
        }
    ]
}